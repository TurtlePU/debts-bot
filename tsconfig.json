{
    "compilerOptions": {
        "target": "esnext",
        "module": "commonjs",
        "outDir": "build",
        "strict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "sourceMap": true,
        "baseUrl": "src",
        "paths": {
            "#/*": [ "*" ]
        }
    }
}

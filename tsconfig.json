{
    "compilerOptions": {
        "target": "esnext",
        "module": "commonjs",
        "outDir": "build",
        "strict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "sourceMap": true,
        "baseUrl": "src",
        "paths": {
            "@bot": [ "bot/bot" ],
            "@locale": [ "locale/export" ],
            "@commands": [ "bot/commands/export" ],
            "@db": [ "db/export" ],
            "@inline": [ "bot/inline/export" ],
            "@util": [ "util/export" ]
        }
    }
}
